<script>

  $.fn.clicktoggle = function(a,b) {
    return this.each(function() {
      var clicked = false;
      $(this).click(function() {
        if (clicked) {
          clicked = false;
          return b.apply(this, arguments);
        }
        clicked = true;
        return a.apply(this, arguments);
      });
    });
  };



  $('[data-toggle="tooltip"]').tooltip();
  $('[data-toggle="popover"]').popover();



  $('.btn').click(function() {
    $(this).children('.text1').toggleClass('hidden');
    $(this).children('.text2').toggleClass('hidden');
  });

